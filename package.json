{
  "name": "eslint-config-gluons",
  "version": "5.0.3",
  "description": "Gluons' personal ESLint shareable config",
  "main": "index.js",
  "files": [
    "./*.js"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "del-cli ./*.js !prettier.config.js",
    "prebuild": "npm run clean",
    "build": "tsc",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,js,vue}\"",
    "pretest": "npm run build",
    "test": "jest",
    "posttest": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/eslint-config-gluons.git"
  },
  "keywords": [
    "❤️",
    "eslint",
    "eslintconfig",
    "gluons"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/eslint-config-gluons/issues"
  },
  "homepage": "https://gluons.github.io/eslint-config-gluons/",
  "dependencies": {
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@gluons/prettier-config": "^2.0.3",
    "@types/eslint": "^7.0.0",
    "@types/jest": "^26.0.7",
    "@types/node": "12",
    "babel-jest": "^26.1.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.5.0",
    "eslint-config-xo": "^0.32.1",
    "jest": "^26.1.0",
    "lodash.merge": "^4.6.1",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "eslint": "7",
    "prettier": "2"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-vue": "^6.2.2"
  }
}
