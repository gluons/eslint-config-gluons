{
  "name": "eslint-config-gluons",
  "version": "6.0.8",
  "description": "Gluons' personal ESLint shareable config.",
  "main": "index.js",
  "typings": "index.d.ts",
  "files": [
    "rules",
    "esnext.js",
    "esnext.d.ts",
    "index.js",
    "index.d.ts",
    "mixed.js",
    "mixed.d.ts",
    "ts.js",
    "ts.d.ts",
    "vue.js",
    "vue.d.ts",
    "vue-ts.js",
    "vue-ts.d.ts"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "del-cli ./*.{js,d.ts}",
    "prebuild": "npm run clean",
    "build": "tsc",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{json,ts,js,vue}\"",
    "pretest": "npm run build",
    "test": "jest",
    "posttest": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/eslint-config-gluons.git"
  },
  "keywords": [
    "❤️",
    "eslint",
    "eslintconfig",
    "gluons"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/eslint-config-gluons/issues"
  },
  "homepage": "https://gluons.github.io/eslint-config-gluons/",
  "prettier": "@gluons/prettier-config",
  "dependencies": {
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@gluons/prettier-config": "^2.0.3",
    "@types/eslint": "^7.2.4",
    "@types/jest": "^26.0.14",
    "@types/node": "^12.12.67",
    "del-cli": "^3.0.1",
    "eslint": "^7.11.0",
    "eslint-config-xo": "^0.33.0",
    "eslint-config-xo-typescript": "^0.34.0",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "tsconfig-gluons": "^1.1.0",
    "typescript": "^4.0.3",
    "vue": "^2.6.12"
  },
  "peerDependencies": {
    "eslint": "7",
    "prettier": "2"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint-plugin-vue": "^7.0.1"
  }
}
