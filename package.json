{
  "name": "eslint-config-gluons",
  "version": "5.0.3",
  "description": "Gluons' personal ESLint shareable config",
  "main": "index.js",
  "files": [
    "./*.js"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "del-cli ./*.js !prettier.config.js",
    "prebuild": "npm run clean",
    "build": "tsc",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,js,vue}\"",
    "pretest": "npm run build",
    "test": "jest",
    "posttest": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/eslint-config-gluons.git"
  },
  "keywords": [
    "❤️",
    "eslint",
    "eslintconfig",
    "gluons"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/eslint-config-gluons/issues"
  },
  "homepage": "https://gluons.github.io/eslint-config-gluons/",
  "dependencies": {
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@gluons/prettier-config": "^2.0.3",
    "@types/eslint": "^6.8.0",
    "@types/jest": "^25.2.1",
    "@types/node": "10",
    "babel-jest": "^25.4.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-xo": "^0.29.1",
    "jest": "^25.4.0",
    "lodash.merge": "^4.6.1",
    "prettier": "^2.0.5",
    "typescript": "~3.8.3"
  },
  "peerDependencies": {
    "eslint": "5 || 6",
    "prettier": "2"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-eslint": "^10.1.0",
    "eslint-plugin-vue": "^6.2.2"
  }
}
